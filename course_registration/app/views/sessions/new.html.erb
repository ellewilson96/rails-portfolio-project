<%= form_for(:teacher, url: login_path) do |f| %>

      <%= f.label :username %>
      <%= f.text_field :username %>

      <%= f.label :password %>
      <%= f.password_field :password %>

      <%= f.submit "Sign In"  %>
    <% end %><br>
<h4>New to Flatiron High School? Create an account <%= link_to "here!", new_teacher_path %></h4>
