<h3>Welcome back, <%= current_user.email %></h3>
  <h4>Your Students</h4>
    <% if @students.empty? %>
    <h4>No students? <p><%= link_to 'Click Here to Add a Student', new_student_path %></p></h4>
    <% else %>
  <% @students.each do |student| %>
<div>
  <h3><%= student.first_name %> <%= student.last_name%></h3>
      <%= link_to "View All Grades", student_grades_path(student) %>
      <% end %></div>
    <% end %>
